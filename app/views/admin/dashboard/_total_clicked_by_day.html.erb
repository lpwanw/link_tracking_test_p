<div class="rounded-lg border border-gray-200 shadow-md p-6 bg-white">
  <h1 class="text-xl font-semibold text-gray-800 mb-4">Total clicked by day</h1>
  <ul class="list-disc list-inside">
    <% total_clicked_by_day.each do |date, count| %>
      <li class="mt-2">
        <%= date %>
        <br>
        <span class="text-sm text-gray-600">Clicks: <%= count %></span>
      </li>
    <% end %>
  </ul>
</div>
